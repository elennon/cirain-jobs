<?xml version="1.0" encoding="UTF-8"?>
<Shell xmlns="http://xamarin.com/schemas/2014/forms" 
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:Extras.Views"
       Title="Extras"
       x:Class="Extras.AppShell">

    <FlyoutItem Title="New Extra">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate local:NewExtra}" />
        </Tab>
    </FlyoutItem>
    <FlyoutItem Title="Veiw all">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate local:VeiwAll}" />
        </Tab>
    </FlyoutItem>
    <FlyoutItem Title="About">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate local:AboutPage}" />
        </Tab>
    </FlyoutItem>

</Shell>
